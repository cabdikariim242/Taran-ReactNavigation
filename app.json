{
  "expo": {
    "name": "TaranAppp",
    "slug": "TaranAppp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/taran.png",
    "scheme": "taranappp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/taran.png",
        "backgroundImage": "./assets/taran.png",
        "monochromeImage": "./assets/taran.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/taran.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/taran.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "jsEngine": "jsc"
  }
}

